    "js-yaml": "^3.11.0",
    "caporal": "^0.10.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/js-yaml": "^3.11.1",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.0.4",
    "dot-json": "^1.0.4",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.5",
    "tslint": "^5.10.0",
